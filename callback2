function logA(a, b, callback) {
  setTimeout(() => {
    console.log("a");
    
    const sum = a*b;    
   
      callback(sum);
  }, 1000);
}

function logB(param) {
  console.log("wynik dzialania funkcji b")
  console.log(param)
};

logA(3,6,logB) 



